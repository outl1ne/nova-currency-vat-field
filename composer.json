{
  "name": "outl1ne/nova-currency-vat-field",
  "description": "A Laravel Nova Currency field extension with VAT toggle.",
  "keywords": [
    "laravel",
    "nova",
    "currency",
    "price",
    "vat",
    "field"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "outl1ne/nova-translations-loader": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\NovaCurrencyVatField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\NovaCurrencyVatField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
